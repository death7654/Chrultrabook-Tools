{
  "build": {
    "beforeDevCommand": "npm run start",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/chrultrabook-tools/browser",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "linux": {
      "appimage": {
        "bundleMediaFramework": false,
        "files": {
          "/user/bin/ectool": "assets/ectool",
	        "/user/bin/cbmem": "assets/cbmem"
        }
      },
      "deb": {
        "files": {
	        "/user/bin/ectool": "assets/ectool",
          "/user/bin/cbmem": "assets/cbmem"
        }
      },
      "rpm": {
        "epoch": 0,
        "files": {
          "/user/bin/ectool": "assets/ectool",
          "/user/bin/cbmem": "assets/cbmem"
        },
        "release": "3.0.5"
      }
    }
  },
  "productName": "chrultrabook-tools",
  "version": "3.0.5",
  "identifier": "com.chrultrabook.org",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Chrultrabook Tools",
        "width": 390,
        "height": 480,
        "maximizable": false,
        "resizable": false,
        "theme": "Dark"
      }
    ]
  }
}
