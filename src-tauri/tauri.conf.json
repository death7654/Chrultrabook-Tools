{
  "build": {
    "beforeDevCommand": "npm run start",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/chrultrabook-tools/browser",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": [
      "deb",
      "rpm",
      "msi",
      "nsis",
      "dmg"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "linux": {
      "deb": {
        "preInstallScript": "src-tauri/scripts/deb_preinstall.sh",
        "provides": ["chromium-ectool", "chromium-cbmem"],
        "postRemoveScript": "src-tauri/scripts/deb_postremove.sh"
      },
      "rpm": {
        "postInstallScript": "src-tauri/scripts/rpm_preinstall.sh",
        "provides": ["cbmem", "chromium-ectool"],
        "postRemoveScript": "src-tauri/scripts/rpm_postremove.sh"
      }
    }
  },
  "productName": "chrultrabook-tools",
  "version": "3.0.7",
  "identifier": "com.chrultrabook.tools",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Chrultrabook Tools",
        "width": 430,
        "height": 480,
        "maximizable": true,
        "resizable": true,
        "theme": "Dark"
      }
    ]
  }
}
